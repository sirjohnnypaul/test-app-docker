    "@types/node": "^20.1.5",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.33",
    "@types/superagent": "4.1.10",
    "@types/supertest": "^2.0.11",
    "jest": "^29.5.0",
    "nodemon": "^1.18.9",
    "prettier": "^2.8.8",
    "supertest": "^3.4.1",
